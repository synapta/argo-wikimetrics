
var TraductionLoop=function(langIndex,liner,callbackEP,LCallback)
{
    console.log("TL"+liner);
    var line;
    if(line=liner.next())
        TraductionExecute(line,liner,langIndex,liner,LCallback)
    else
    {
        console.log(LCallback);
        LCallback(langIndex,callbackEP);
    }   

}
var TraductionExecute=function(line,liner,langIndex,LCallback)
{
    //Readline
    var RQ="";
    var i=0;
    do
    {
        if(i!=0)
            RQ+=",";
        RQ+=("\""+decodeURI(line.toString('ascii')+"\""));
        i++;
    }while(i<traductionLines&&(line=liner.next()))
    if(RQ.length>5)
    {
        Query(langIndex,liner,LCallback,
            function(langIndex,liner,LCallback)
            {
                TraductionLoop(langIndex,liner,LCallback);
            }
        )
    }
    TraductionLoop(langIndex,liner,LCallback);
}


var Query=function(langIndex,liner,LCallback,callback)
{
    callback(langIndex,liner,LCallback);
}